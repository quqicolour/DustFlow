#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/blockchain/personal_project/DustFlow/node_modules/.store/ts-node@10.9.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/blockchain/personal_project/DustFlow/node_modules/.store/ts-node@10.9.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/ts-node@10.9.2/node_modules/ts-node/dist/bin-cwd.js" "$@"
else
  exec node  "$basedir/../.store/ts-node@10.9.2/node_modules/ts-node/dist/bin-cwd.js" "$@"
fi
